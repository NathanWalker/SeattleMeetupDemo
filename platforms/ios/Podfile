use_frameworks!

target "SeattleMeetup" do
# Begin Podfile - /Users/nathan/Documents/github/NathanWalker/demo/SeattleMeetup/node_modules/nativescript-pdf/platforms/ios/Podfile 
 pod 'SimplePDF', :git => 'https://github.com/NathanWalker/SimplePDF.git'

post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['SWIFT_VERSION'] = '3.0'
    end
  end
end 
 # End Podfile 

end